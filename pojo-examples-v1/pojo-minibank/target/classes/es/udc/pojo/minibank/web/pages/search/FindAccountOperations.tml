<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	t:type="Layout" t:title="title">
	
<form t:type="Form" class="form-horizontal" t:id="findAccountOperationsByDateForm">

	<t:errors/>

    <div class="form-group">
    	<t:label for="accountId" class="col-md-offset-3 col-md-2"/>
        <div class="col-md-3">
            <input t:type="TextField" t:id="accountId" 
            	   t:validate="required, min=0" size="16" maxlength="16"/>
        </div>
    </div>
    
     <div class="form-group">
    	<t:label for="startDate" class="col-md-offset-3 col-md-2"/>
        <div class="col-md-3">
            <input t:type="TextField" t:id="startDate" t:validate="required"
            	size="10" maxlength="10"/>
        </div>
    </div>
    
    <div class="form-group">
    	<t:label for="endDate" class="col-md-offset-3 col-md-2"/>
        <div class="col-md-3">
            <input t:type="TextField" t:id="endDate" t:validate="required"
            	size="10" maxlength="10"/>
        </div>
    </div>
    
    <div class="form-group">
    	<div class="col-md-offset-5 col-md-1">
			<button type="submit" class="btn btn-primary">${message:button-find}</button>
		</div>
    </div>
    
</form>

</html>
